# Frontend Environment Variables (Vite)
# Copy this file to .env.local and fill in your values

# Socket.io Server URL (for online multiplayer mode)
# In development: http://localhost:3002
# In production: Your Fly.io app URL (e.g., https://akong-server.fly.dev)
VITE_SOCKET_SERVER_URL=http://localhost:3002

# Gemini API Key (currently not used in the game logic)
GEMINI_API_KEY=
